{
    "name": "degraciamathieu/php-wording-detector",
    "description": "check your DDD approach by analyzing your variables",
    "keywords": ["php", "quality", "ci", "metrics", "static-analyzer", "ddd"],
    "type": "package",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/DeGraciaMathieu/php-wording-detector/issues",
        "source": "https://github.com/DeGraciaMathieu/php-wording-detector"
    },
    "authors": [
        {
            "name": "De Gracia Mathieu",
            "email": "dev@degracia-mathieu.fr"
        }
    ],
    "require": {
        "php": "^8.1",
        "degraciamathieu/php-file-explorer": "0.4.*",
        "illuminate/view": "^10.0",
        "laravel-zero/framework": "^10.0",
        "nikic/php-parser": "^4.13",
        "nunomaduro/termwind": "^1.15"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.22.3",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["php-wording-detector"]
}
